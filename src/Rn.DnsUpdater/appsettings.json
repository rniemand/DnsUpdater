        "Username": "guest",
        "Password": "guest",
        "VirtualHost": "/",
        "Host": "127.0.0.1",
        "Port": 5672,
        "Exchange": "amq.topic",
        "RoutingKey": "metrics.home",
        "BackOffTimeSec": 15,
        "CoolDownTimeSec": 300,
        "CoolDownThreshold": 3,
        "MaxCoolDownRuns": 0
      }
    }
  }
