{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "DnsUpdater": {
    "ConfigFile": "D:\\wrk\\[docker]\\DnsUpdater\\config\\dns.config.json",
    "TickInterval": 5000,
    "UpdateHostIpIntervalMin": 10,
